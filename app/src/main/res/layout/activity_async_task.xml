<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Background.AsyncTaskActivity">

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_marginStart="16dp"
        android:layout_marginTop="16dp" android:onClick="startAsyncTask"
        android:text="no option" app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:layout_marginStart="26dp"
        android:layout_marginTop="16dp" android:onClick="startAsyncTaskWithTpe"
        android:text="ThreadPool"
        app:layout_constraintStart_toEndOf="@+id/button5" app:layout_constraintTop_toTopOf="parent"/>
</android.support.constraint.ConstraintLayout>